    <script src="/scripts/main.js"></script>
    

  </head>
  <body>

  	<form action="/spots" method="POST">
  		<input type="text" placeholder="spot" name="name">
  		<input type="text" placeholder="description" name="description">
  		<button type="submit">Submit</button>
	</form>
	<div id='spotlist'>
	</div>	
	<% for (i=0; i<spots.length; i++) { %>
	<span> <%= spots[i].name %> <%= spots[i].description %> </span><button type="button" name="button" id= `<%= spots[i]._id %>`>delete</button><br>
	
	<% } %>
  </body>
</html>